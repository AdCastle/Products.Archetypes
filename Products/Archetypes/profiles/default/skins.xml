<?xml version="1.0"?>
<object name="portal_skins" meta_type="Plone Skins Tool" allow_any="False"
   cookie_persistence="False" default_skin="Plone Default"
   request_varname="plone_skin">
 <object name="archetypes" meta_type="Filesystem Directory View"
    directory="Products.Archetypes:skins/archetypes"/>
 <object name="archetypes_kss" meta_type="Filesystem Directory View"
    directory="Products.Archetypes:skins/archetypes_kss"/>
  <skin-path name="*">
  <layer name="archetypes" insert-after="custom"/>
  <layer name="archetypes_kss" insert-after="archetypes"/>
 </skin-path>
</object>
